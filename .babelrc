{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["formatjs", {
        "ast": true
    }],
    "transform-object-assign",
    "transform-flow-strip-types",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-react-inline-elements"
      ]
    }
  }
}
